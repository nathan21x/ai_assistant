{
    "version": 2,
    "builds": [
        {
            "src": "api/server.js",
            "use": "@vercel/node"
        },
        {
            "src": "next.config.ts",
            "use": "@vercel/next"
        }
    ],
    "routes": [
        {
            "src": "/api/(.*)",
            "dest": "api/server.js"
        },
        {
            "src": "frontend/(.*)",
            "dest": "frontend/$1"
        }
    ]
}
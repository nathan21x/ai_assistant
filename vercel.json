{
    "version": 2,
    "builds": [
        {
            "src": "api/server.js",
            "use": "@vercel/node"
        },
        {
            "src": "frontend/next.config.js",
            "use": "@vercel/next"
        }
    ],
    "routes": [
        {
            "src": "/api/(.*)",
            "dest": "api/server.js"
        },
        {
            "src": "/frontend/(.*)",
            "dest": "$1"
        }
    ]
}